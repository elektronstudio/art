{
  "name": "live2",
  "private": true,
  "scripts": {
    "dev": "vite -c vite.config.app.js",
    "build": "vite build -c vite.config.app.js && vite build -c vite.config.lib.js --outDir dist/lib",
    "serve": "vite preview"
  },
  "dependencies": {
    "hls.js": "^0.14.17",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.19.0",
    "eslint-plugin-vue": "^7.5.0",
    "vite": "^2.0.0-beta.65"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
